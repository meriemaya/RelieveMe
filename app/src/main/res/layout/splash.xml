<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/screen_percentage_12"
    android:layout_marginTop="@dimen/screen_percentage_12"
    android:layout_marginBottom="@dimen/screen_percentage_12"
    android:layout_marginRight="@dimen/screen_percentage_12"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/splash_logo"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/_"
        android:layout_marginTop="@dimen/screen_percentage_05"
        tools:src="@mipmap/ic_launcher"
        android:contentDescription="Logo" />

    <TextView
        android:id="@+id/title_logo"
        android:text="@string/title_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/splash_logo"
        android:layout_marginTop="@dimen/screen_percentage_05"
        tools:text="@string/title_logo"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
